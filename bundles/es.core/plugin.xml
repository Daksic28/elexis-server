<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="elexis-server"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="info.elexis.server.core.internal.Application">
         </run>
      </application>
   </extension>
   <extension
         id="ElexisServer"
         point="org.eclipse.core.runtime.products">
      <product
            application="info.elexis.server.core.elexis-server"
            name="elexis-server">
         <property
               name="appName"
               value="elexis-server">
         </property>
      </product>
   </extension>

</plugin>
